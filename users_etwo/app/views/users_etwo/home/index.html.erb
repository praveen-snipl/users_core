<div class="container">
	<p id="notice"><%= notice %></p>
	<div class="col-md-12 home_page">
		<div class="col-md-4">
			<a href="/" class="btn btn-default">Home</a>
		</div>
		<div class="col-md-4">
			<a href="/users_eone" class="btn btn-default">Engine 1</a>
		</div>
		<div class="col-md-4">
			<a href="/users_etwo" class="btn btn-info">Engine 2</a>
		</div>
	</div>
	<div class="row users_list">
		<h1>Action cached pages</h1>
			<div class="col-md-12">
				<div class="col-md-3">Id</div>
				<div class="col-md-3">Name</div>
				<div class="col-md-3">Email</div>
				<div class="col-md-3">Udated At</div>
			</div>
		<% @users.each do|user| %>
			<div class="col-md-12">
				<div class="col-md-3"><%= user.id %></div>
				<div class="col-md-3"><a href="<%= main_app.user_path(user) %>"><%= user.name %></a></div>
				<div class="col-md-3"><%= user.email %></div>
				<div class="col-md-3"><%= user.updated_at %>(<a href="<%= main_app.edit_user_path(user) %>">Edit</a>)</div>
			</div>
		<% end %>		
		<div class="users_page_links">
			<%= paginate @users %>
		</div>	
	</div>
</div>