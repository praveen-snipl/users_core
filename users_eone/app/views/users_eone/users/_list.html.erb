<div class="col-md-3">Id</div>
<div class="col-md-3">Name</div>
<div class="col-md-3">Email</div>
<div class="col-md-3">Udated At</div>	
<% users.each do|user| %>
	<div class="col-md-12">
		<div class="col-md-3"><%= user.id %></div>
		<div class="col-md-3"><a href="<%= main_app.user_path(user) %>"><%= user.name %></a></div>
		<div class="col-md-3"><%= user.email %></div>
		<div class="col-md-3"><%= user.updated_at %>(<a href="<%= main_app.edit_user_path(user) %>" data-remote="true" class="edit_user_eone" data-user-id="<%= user.id %>">Edit</a>)</div>
	</div>
<% end %>
<input type="hidden" name="current_page" value="<%= current_display_page %>" id="current_page" />
<div class="users_page_links">
	<%= paginate @users %>
</div>