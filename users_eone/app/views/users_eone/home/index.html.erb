<div class="container">
	<p id="notice"><%= notice %></p>
	<div class="col-md-12 home_page">
		<div class="col-md-4">
			<a href="/" class="btn btn-default">Home</a>
		</div>
		<div class="col-md-4">
			<a href="/users_eone" class="btn btn-info">Engine 1</a>
		</div>
		<div class="col-md-4">
			<a href="/users_etwo" class="btn btn-default">Engine 2</a>
		</div>
	</div>
	<div class="row users_list">
			<h1>Fragment Caching done for the products listing.</h1>

			<div class="col-md-12" style="width:70%;">
				<% cache [ 'users_eone_engine', User.order(:updated_at => 'desc').first, current_display_page ] do %>
					<%= render partial: 'users_eone/users/list', locals: { users: @users } %>
				<% end %>
			</div>
			<div class="col-md-12" style="width:30%;border: 1px solid;padding: 30px;background-color:#95a5a6;" id="user_edit_eone">
			</div>
	</div>
</div>